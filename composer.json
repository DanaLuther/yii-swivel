{
    "name": "dhluther/yii-swivel",
    "type": "yii-extension",
    "description": "A Yii plugin to provide easy access and integration with zumba/swivel",
    "keywords": ["yii", "extension", "swivel", "helper", "component", "behavior"],
    "homepage": "https://github.com/DanaLuther/yii-swivel",
    "license": "MIT",
    "authors": [
        {
            "name": "Dana Luther",
            "homepage": "http://dhluther.lutherweb.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "composer/installers": "1.0.*",
        "zumba/swivel": "2.*"
    },
    "support": {
        "source": "https://github.com/DanaLuther/yii-swivel"
    },
    "config": {
        "vendor-dir": "vendor"
    },
    "extra": {
        "installer-name": "Swivel"
    }
}